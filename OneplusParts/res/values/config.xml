<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Device earpiece gain sysfs -->
    <string name="pathAudioEarpieceGain" />

    <!-- Device earpiece gain min-max-default values -->
    <integer name="audioEarpieceMin" />
    <integer name="audioEarpieceMax" />
    <integer name="audioEarpieceInterval">1</integer>
    <integer name="audioEarpieceDefault" />

    <!-- Device audio mic gain sysfs -->
    <string name="pathAudioMicGain" />

    <!-- Device audio mic min-max-default values -->
    <integer name="audioMicGainMin" />
    <integer name="audioMicGainMax" />
    <integer name="audioMicGainInterval">1</integer>
    <integer name="audioMicGainDefault" />

    <!-- Device audio headphone gain sysfs -->
    <string name="pathAudioHeadphoneGain" />

    <!-- Device audio headphone min-max-default values -->
    <integer name="audioHeadphoneGainMin" />
    <integer name="audioHeadphoneGainMax" />
    <integer name="audioHeadphoneInterval">1</integer>
    <integer name="audioHeadphoneGainDefault" />

    <!-- Device audio speaker gain sysfs -->
    <string name="pathAudioSpeakerGain" />

    <!-- Device audio speaker min-max-default values -->
    <integer name="audioSpeakerGainMin" />
    <integer name="audioSpeakerGainMax" />
    <integer name="audioSpeakerInterval">1</integer>
    <integer name="audioSpeakerGainDefault" />

    <!-- Device fps path -->
    <string name="pathfpsInfo" />

    <!-- Default action ids for a device's touchscreen gestures
         The array should be sorted in the same order as the
         touchscreen gestures advertised by the device's LineageHW impl. -->
    <integer-array name="config_defaultTouchscreenGestureActions" />
</resources>
